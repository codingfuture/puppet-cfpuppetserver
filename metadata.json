{
  "name": "codingfuture-cfpuppetserver",
  "version": "0.10.5",
  "author": "Andrey Galkin",
  "summary": "Alternative PuppetDB+PostgreSQL and PuppetServer setup module",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cfpuppetserver",
  "project_page": "https://github.com/codingfuture/puppet-cfpuppetserver",
  "issues_url": "https://github.com/codingfuture/puppet-cfpuppetserver/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "15.10",
        "16.04",
        "16.10"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.8.0 <5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "codingfuture-cfsystem",
      "version_requirement": ">= 0.10.1 <0.11.0"
    },
    {
      "name": "codingfuture-cfdb",
      "version_requirement": ">= 0.10.1 <0.11.0"
    }
  ],
  "data_provider": "hiera"
}
